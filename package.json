{
  "name": "rotki.com",
  "version": "1.12.0",
  "private": true,
  "packageManager": "pnpm@8.15.3",
  "type": "module",
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi typecheck && nuxi build",
    "start": "nuxi start",
    "generate": "nuxi generate",
    "postinstall": "nuxi prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:style": "stylelint **/*.{vue,css,scss} --ignore-path .gitignore",
    "lint:all": "pnpm lint && pnpm lint:style",
    "typecheck": "npx nuxi typecheck",
    "test": "vitest run",
    "test:watch": "vitest --ui",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --headless",
    "coverage": "vitest run --coverage",
    "prepare": "husky",
    "release": "bumpp -r --no-push"
  },
  "dependencies": {
    "@metamask/detect-provider": "2.0.0",
    "@nuxtjs/robots": "3.0.0",
    "@nuxtjs/tailwindcss": "6.11.4",
    "@pinia/nuxt": "0.5.1",
    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.4",
    "@vueuse/core": "10.7.2",
    "@vueuse/math": "10.7.2",
    "@vueuse/nuxt": "10.7.2",
    "@vueuse/shared": "10.7.2",
    "braintree-web": "3.100.0",
    "cypress": "13.6.4",
    "ethers": "6.11.1",
    "loglevel": "1.9.1",
    "pinia": "2.1.7",
    "qrcode": "1.5.3",
    "swiper": "11.0.6",
    "vue": "3.4.5",
    "vue-router": "4.2.5",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "18.6.2",
    "@fontsource/roboto": "5.0.8",
    "@nuxt/content": "2.12.0",
    "@nuxt/devtools": "1.0.8",
    "@nuxt/test-utils": "3.11.0",
    "@nuxtjs/i18n": "8.1.1",
    "@nuxtjs/sitemap": "5.1.0",
    "@rotki/eslint-config": "2.4.5",
    "@rotki/ui-library": "0.13.0",
    "@types/braintree-web": "3.96.11",
    "@types/paypal-checkout-components": "4.0.8",
    "@types/qrcode": "1.5.5",
    "@vitest/coverage-v8": "1.3.0",
    "@vue/test-utils": "2.4.4",
    "autoprefixer": "10.4.17",
    "bumpp": "9.3.0",
    "eslint": "8.56.0",
    "eslint-plugin-nuxt": "4.0.0",
    "happy-dom": "12.10.3",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "msw": "2.2.1",
    "nuxt": "3.9.1",
    "postcss": "8.4.35",
    "postcss-html": "1.6.0",
    "postcss-import": "16.0.1",
    "postcss-url": "10.1.3",
    "sass": "1.71.0",
    "sass-loader": "14.1.1",
    "stylelint": "16.2.1",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard": "36.0.0",
    "stylelint-config-standard-scss": "13.0.0",
    "stylelint-order": "6.0.4",
    "stylelint-scss": "6.1.0",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
    "vite": "5.1.3",
    "vitest": "1.3.0",
    "vitest-environment-nuxt": "1.0.0",
    "vue-tsc": "1.8.11",
    "webpack": "5.90.3"
  },
  "engines": {
    "node": ">=20 <21",
    "pnpm": ">=8 <9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue,md,json,yml}": "eslint",
    "*.{css,vue}": "stylelint"
  }
}
